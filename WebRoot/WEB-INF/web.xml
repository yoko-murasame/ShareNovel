<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>ShareNovel</display-name>
  <servlet>
    <servlet-name>TestServlet</servlet-name>
    <servlet-class>cn.dmdream.controller.TestServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>RegisterAndLogin</servlet-name>
    <servlet-class>cn.dmdream.controller.registerAndLogin.RegisterAndLogin</servlet-class>
  </servlet>


  <servlet-mapping>
    <servlet-name>TestServlet</servlet-name>
    <url-pattern>/testServlet.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>RegisterAndLogin</servlet-name>
    <url-pattern>/rl.do</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>mainpage.jsp</welcome-file>
  </welcome-file-list>
</web-app>